PORT=3000
LOG_LEVEL=debug
ALL_SCHEMA_ON=true
ENVIRONMENT= # Set to name of environment
DAL_REQUEST_TIMEOUT_MS=3000
GRAPHQL_DASHBOARD_ENABLED=true

# Kits Gateway for RP (Locally generated self-signed cert and key for testing)
KITS_GATEWAY_INTERNAL_URL=https://rp_kits_gateway_internal_url/v1/
KITS_GATEWAY_TIMEOUT_MS=3000
# cspell:disable-next-line
KITS_CONNECTION_CERT="" #pragma: allowlist secret
# cspell:disable-next-line
KITS_CONNECTION_KEY="" #pragma: allowlist secret

# Generate and validate api bearer token
OIDC_JWKS_URI=http://mock-jwks-endpoint
OIDC_JWKS_TIMEOUT_MS=3000
ADMIN_AD_GROUP_ID=some-ad-group-id
CLIENT_ID=some-client-id
CLIENT_SECRET=some-client-secret

# Health checks
HEALTH_CHECK_ENABLED=true                             # Enable health checks
HEALTH_CHECK_RP_INTERNAL_ORGANISATION_ID=some-org-id  # Organisation ID for health check
HEALTH_CHECK_RURAL_PAYMENTS_THROTTLE_TIME_MS=300000   # Only check rural payments every 5 minutes

# Acceptance tests
ACCEPTANCE_TEST_ENVIRONMENT_URL= # Set to url of environment to run acceptance tests against
ACCEPTANCE_TEST_RP_INTERNAL_USER_EMAIL=
ACCEPTANCE_TEST_RP_INTERNAL_ORGANISATION_SBI=
ACCEPTANCE_TEST_RP_INTERNAL_CUSTOMER_CRN=
ACCEPTANCE_TEST_AUTHENTICATE_USER_OBJECT_ID=
